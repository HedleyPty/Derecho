tmp$sexo<-"todos"
tmp$sexo[39:76]<-"hombres"
tmp$sexo[77:114]<-"mujeres"
tmp$edad[c(39,77)]<-"TOTAL"
tmp$provincia<-provincias[i]
poblacion.provincial<-rbind(poblacion.provincial, tmp)
file.remove(file.)
rm(list=c(grep("tmp", ls(), value=T), "file."))
}
